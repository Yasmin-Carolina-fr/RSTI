const readline = require('readline');
const rl = readline.createInterface({
  input: process.stdin,
  output: process.stdout
});
rl.question('informe a velocidade do carro: ', (velocidade) => {
  if (parseInt(velocidade) > 80) {
    const multa = (parseInt(velocidade) - 80) * 5;
    console.log(multa);
  } else {
    console.log('n√£o foi multado');
  }
  rl.close();
});
